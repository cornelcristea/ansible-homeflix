<?xml version="1.0" encoding="utf-8"?>
<BrandingOptions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
{% if homeflix_jellyfin_login_disclaimer %}
  <LoginDisclaimer>
    {{ homeflix_jellyfin_login_disclaimer }}
  </LoginDisclaimer>
{% else %}
  <LoginDisclaimer />
{% endif %}
{% if homeflix_jellyfin_custom_css %}
  <CustomCss>
    {{ homeflix_jellyfin_custom_css }}
  </CustomCss>
{% else %}
  <CustomCss />
{% endif %}
  <SplashscreenEnabled>{{ homeflix_jellyfin_splash_screen_enabled }}</SplashscreenEnabled>
</BrandingOptions>

